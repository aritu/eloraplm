            </layouts>
            <layouts mode="edit">
                <layout>heading</layout>
                <layout>dublincore</layout>
            </layouts>
            <contentViews category="content">
                <contentView>structure_document_content</contentView>
            </contentViews>
            <subtypes>
                <type>Folder</type>
                <type>OrderedFolder</type>
                <type>TemplateSource</type>
                <type>WebTemplateSource</type>
            </subtypes>
        </type>
      
        <type id="TemplateSource">
            <icon>/icons/template_source_16.png</icon>
            <bigIcon>/icons/template_source_100.png</bigIcon>
        </type>
        
        <type id="Root">
