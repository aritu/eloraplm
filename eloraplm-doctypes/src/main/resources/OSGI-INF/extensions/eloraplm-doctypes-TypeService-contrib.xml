    </doctype>
    
    <doctype extends="Folder" name="FolderCadPart">
      <facet name="EloraStruct"/>
    </doctype>

    <doctype extends="Folder" name="FolderCadAssembly">
      <facet name="EloraStruct"/>
    </doctype>
    
    <doctype extends="Folder" name="FolderCadDrawing">
      <facet name="EloraStruct"/>
