                <type>BomSpecification</type>
                <type>BomProduct</type>
                <type>BomCustomerProduct</type>
                <type>Software</type>
                <type>ElectronicComponent</type>
                <type>CadAssembly</type>
                <type>CadPart</type>
                <type>CadDrawing</type>
                <type>CadDesignTable</type>
