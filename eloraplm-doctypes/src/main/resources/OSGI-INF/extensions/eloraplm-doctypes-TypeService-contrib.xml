                <layout>dublincore</layout>
            </layouts>
            <subtypes>
                <type>CadAssembly</type>
                <type>CadPart</type>
                <type>CadDrawing</type>
                <type>CadDesignTable</type>
            </subtypes>
            <deniedSubtypes>
                <type>Workspace</type>
            </deniedSubtypes>
        </type>
        <type id="Folder">
            <icon>/icons/folder_16.png</icon>
            <bigIcon>/icons/folder_100.png</bigIcon>
