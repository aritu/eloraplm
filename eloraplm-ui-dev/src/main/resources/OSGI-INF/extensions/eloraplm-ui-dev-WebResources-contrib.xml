<?xml version="1.0"?>
<component name="com.aritu.eloraplm.ui.dev.WebResources.contrib">
    <require>com.aritu.eloraplm.ui.WebResources.contrib</require>

    <extension target="org.nuxeo.ecm.platform.WebResources"
        point="resources">
        <resource name="eloraplm.dev.style.css" type="css">
            <uri>/css/eloraplm-dev-style.css</uri>
        </resource>
    </extension>

    <extension target="org.nuxeo.ecm.platform.WebResources"
        point="bundles">
        <bundle name="nuxeo_sassCss">
            <resources append="true">
                <resource>eloraplm.dev.style.css</resource>
            </resources>
        </bundle>
    </extension>

</component>