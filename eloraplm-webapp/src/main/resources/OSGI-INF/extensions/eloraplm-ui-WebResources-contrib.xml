<?xml version="1.0"?>
<component name="com.aritu.eloraplm.ui.WebResources.contrib">
    <require>org.nuxeo.theme.nuxeo.webapp</require>

    <extension target="org.nuxeo.ecm.platform.WebResources"
        point="resources">
        <resource name="eloraplm.style.css" type="css">
            <uri>/css/eloraplm-style.css</uri>
        </resource>
    </extension>
    
    <extension target="org.nuxeo.ecm.platform.WebResources"
        point="bundles">
        <bundle name="nuxeo_sassCss">
            <resources append="true">
                <resource>eloraplm.style.css</resource>
            </resources>
        </bundle>
    </extension>
    
</component>