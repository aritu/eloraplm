<?xml version="1.0"?>

<component name="com.aritu.eloraplm.lifecycles.SQLDirectoryFactory.contrib">

  <extension target="org.nuxeo.ecm.directory.sql.SQLDirectoryFactory" point="directories">
    
    <directory name="elora_children_supported_states">
      <schema>eloraChildrenSupportedStates</schema>
      <dataSource>java:/nxsqldirectory</dataSource>
      <table>elora_children_support_states</table>
      <idField>id</idField>
      <autoincrementIdField>false</autoincrementIdField>
      <readOnly>false</readOnly>
      <dataFile>directories/elora_children_supported_states.csv</dataFile>
      <createTablePolicy>always</createTablePolicy>
    </directory>
    
    <directory name="elora_lifecycle_transitions">
      <schema>eloraLifecycleTransitions</schema>
      <dataSource>java:/nxsqldirectory</dataSource>
      <table>elora_lifecycle_transitions</table>
      <idField>id</idField>
      <autoincrementIdField>false</autoincrementIdField>
      <readOnly>false</readOnly>
      <dataFile>directories/elora_lifecycle_transitions.csv</dataFile>
      <createTablePolicy>always</createTablePolicy>
    </directory>
    
    <directory name="elora_transition_config">
      <schema>eloraTransitionConfig</schema>
      <dataSource>java:/nxsqldirectory</dataSource>
      <table>elora_transition_config</table>
      <idField>id</idField>
      <autoincrementIdField>false</autoincrementIdField>
      <readOnly>false</readOnly>
      <dataFile>directories/elora_transition_config.csv</dataFile>
      <createTablePolicy>always</createTablePolicy>
    </directory>
    
  </extension>

</component>